<script>
    $(document).ready(function() {
    $('#all_tissues').dataTable( {
        "paging":   true,
        "ordering": true,
        "info":     false,
        searching: false,
        data: ${tissues},
                columns: [
                    { data: "tissue" ,
                    "render": function ( data, type, row ) {
                        return '<a href="organ/'+data+'">'+ data+ '</a>';
                    }},
                        { data: "tissue_count" ,
                    "render": function ( data, type, row ) {
                        return data;
                    }}
                ]
            } );
        } );
</script>

